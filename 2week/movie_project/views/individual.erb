  	<li>Rating: <%= @individual_movie['Rated']%></li>
  	<li>Runtime: <%= @individual_movie['Runtime']%></li>
  	<li>Director(s): <%= @individual_movie['Director']%></li>
  	<li>Actor(s):<%= @individual_movie['Actors']%></li>


  </ul>


</div>
<div class="col-sm-1 col-md-1 col-lg-1"></div>
</div>

