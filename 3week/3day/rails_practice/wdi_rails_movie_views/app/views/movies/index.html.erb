<div class=index>
<h1>Rails Movie App</h1>

<% @movies.each do |movie| %>

  <!-- <a href="/movies/<%#= movie['imdbID'] %>"><%#= movie["Title"] %></a> - <%#= movie["Year"] %></br> -->

  <%= link_to movie["title"], movie_path(movie.id) %> - <%= movie["year"] %></br>

  <%= button_to "delete", {controller: :movies, action: :destroy, id: movie.id }, :method => :delete %>
  <%= button_to "edit", {controller: :movies, action: :edit, id: movie.id }, :method => :get %>

  <br>

<% end %>
<div>
